-- Create Sequences for all tables with ID columns
CREATE SEQUENCE users_seq START WITH 1 INCREMENT BY 1 MAXVALUE 9999999999;
CREATE SEQUENCE partnerships_seq START WITH 1100 INCREMENT BY 1;
CREATE SEQUENCE stores_seq START WITH 2100 INCREMENT BY 1;
CREATE SEQUENCE memberships_seq START WITH 1000000000000000 INCREMENT BY 1 MAXVALUE 9999999999999999;
CREATE SEQUENCE rewards_seq START WITH 54100 INCREMENT BY 1;
CREATE SEQUENCE coupons_seq START WITH 45100 INCREMENT BY 1;
CREATE SEQUENCE transactions_seq START WITH 303 INCREMENT BY 1;
CREATE SEQUENCE notifications_seq START WITH 909 INCREMENT BY 1;
CREATE SEQUENCE consents_seq START WITH 111100 INCREMENT BY 1;
CREATE SEQUENCE scan_codes_seq START WITH 333333 INCREMENT BY 1;
CREATE SEQUENCE points_batches_seq START WITH 777 INCREMENT BY 1;
CREATE SEQUENCE redemptions_seq START WITH 999909 INCREMENT BY 1;
CREATE SEQUENCE points_spent_seq START WITH 501 INCREMENT BY 1;


