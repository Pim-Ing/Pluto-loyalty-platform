-- Create PARTNERSHIPS Table--
CREATE TABLE PARTNERSHIPS (
    partnership_id NUMBER(10) CONSTRAINT partnerships_partnership_id_pk PRIMARY KEY,
    partner_name VARCHAR2(100) CONSTRAINT partnerships_partner_name_nn NOT NULL,
    contact_email VARCHAR2(100) CONSTRAINT partnerships_contact_email_nn NOT NULL
        CONSTRAINT partnerships_contact_email_u UNIQUE,
    partner_logo_url VARCHAR2(255),
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP CONSTRAINT partnerships_created_at_nn NOT NULL
);

